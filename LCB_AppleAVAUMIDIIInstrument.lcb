            put tNumber formatted as string &"-"& tStr & "\n" after rReturnString
         end repeat
      end if

      -- put c_AudioUnitGetPropertyFactoryPresets(tAudioUnit,tAudioUnitProperty,tAudioUnitScope,tAudioUnitElement,tDataPtr,rDataSize) into tOSStatus
